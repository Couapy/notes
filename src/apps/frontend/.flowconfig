[ignore]
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/public/.*
<PROJECT_ROOT>/src/App.test.js
<PROJECT_ROOT>/src/serviceWorker.js
<PROJECT_ROOT>/src/setupTests.js
<PROJECT_ROOT>/src/archives/.*
<PROJECT_ROOT>/src/svg/.*

[include]
./src/*.js
./src/*.jsx

[libs]
<PROJECT_ROOT>/flow-typed
[lints]
#all=off # default
all=warn

[options]
#module.name_mapper='^\([^\.].*\)$' -> '<PROJECT_ROOT>/src/\1' // For absolute import
emoji=true
include_warnings=true
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
[strict]
